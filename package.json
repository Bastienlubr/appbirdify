{
  "name": "appbirdify",
  "version": "1.0.0",
  "description": "A new Flutter project.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "emu:start": "firebase emulators:start --only firestore",
    "emu:start:background": "powershell -ExecutionPolicy Bypass -File scripts/start-emulator.ps1",
    "emu:ping": "node scripts/ping_emulator.mjs",
    "check": "node scripts/check_environment.mjs",
    "java:install": "powershell -ExecutionPolicy Bypass -File scripts/install_java.ps1",
    "import:oiseaux": "node scripts/import-oiseaux.mjs",
    "import:missions:final": "node scripts/import-missions-final.mjs",
    "import:missions:direct": "node scripts/import-missions-direct-id.mjs",
    "approve:missions": "node scripts/approve-missions.mjs",
    "import:all": "node scripts/import-all.mjs",
    "import:verify": "node scripts/import-all.mjs --verify",
    "missions:creer-ou-completer-progression": "node scripts/missions/creer_ou_completer_progression_utilisateur.mjs",
    "missions:creer-ou-completer-progression:apply": "node scripts/missions/creer_ou_completer_progression_utilisateur.mjs --apply",
    "quiz:remplir-id-oiseau": "node scripts/quiz/fill_species_ids.mjs",
    "quiz:remplir-id-oiseau:apply": "node scripts/quiz/fill_species_ids.mjs --apply",
    "quiz:generer-mauvaises-reponses": "node scripts/quiz/generer_mauvaises_reponses_quiz.mjs",
    "quiz:generer-mauvaises-reponses:export": "node scripts/quiz/generer_mauvaises_reponses_quiz.mjs --out=distracteurs.json",
    "quiz:normaliser-ids": "node scripts/quiz/normalize_quiz_ids.mjs",
    "quiz:publier": "node scripts/quiz/publier_quiz_vers_firestore.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bastienlubr/appbirdify.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bastienlubr/appbirdify/issues"
  },
  "homepage": "https://github.com/Bastienlubr/appbirdify#readme",
  "dependencies": {
    "csv-parse": "^6.1.0",
    "firebase-admin": "^13.4.0"
  }
}
